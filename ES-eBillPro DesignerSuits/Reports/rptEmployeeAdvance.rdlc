      <Query>
        <DataSourceName>GSASDataConnectionString</DataSourceName>
        <CommandText>SELECT        A.Id, A.AdvanceDate, A.EmployeeCode, A.Amount, A.Remarks, M.EmployeeName
FROM            (tblEmployeeAdvance A INNER JOIN
                         tblEmployeeMaster M ON A.EmployeeCode = M.EmployeeCode)
ORDER BY A.AdvanceDate DESC, A.EmployeeCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
