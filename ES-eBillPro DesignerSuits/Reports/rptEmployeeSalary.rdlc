      <Query>
        <DataSourceName>GSASDataConnectionString</DataSourceName>
        <CommandText>SELECT        S.Id, S.SalaryDate, S.SalaryMonth, S.SalaryYear, S.EmployeeCode, S.Salary, S.AbsentDays, S.AbsentAmount, S.WorkingDays, S.WorkingAmount, 
                         S.AdvanceDeducted, S.TotalAmount, S.Remarks, M.EmployeeName
FROM            (tblEmployeeSalary S INNER JOIN
                         tblEmployeeMaster M ON S.EmployeeCode = M.EmployeeCode)
ORDER BY S.SalaryDate DESC, S.EmployeeCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
